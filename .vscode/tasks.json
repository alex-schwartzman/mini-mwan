{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build mini-mwan package",
			"type": "shell",
			"command": "make",
			"args": [
				"package/mini-mwan/compile",
				"V=s"
			],
			"options": {
				"cwd": "/builder"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": [],
			"presentation": {
				"reveal": "always",
				"panel": "new"
			}
		},
		{
			"label": "Clean mini-mwan package",
			"type": "shell",
			"command": "make",
			"args": [
				"package/mini-mwan/clean",
				"V=s"
			],
			"options": {
				"cwd": "/builder"
			},
			"problemMatcher": []
		},
		{
			"label": "List built packages",
			"type": "shell",
			"command": "find",
			"args": [
				"/builder/bin/packages",
				"-name",
				"mini-mwan*.ipk",
				"-exec",
				"ls",
				"-lh",
				"{}",
				";"
			],
			"problemMatcher": [],
			"presentation": {
				"reveal": "always"
			}
		},
		{
			"label": "Build and list packages",
			"dependsOrder": "sequence",
			"dependsOn": [
				"Build mini-mwan package",
				"List built packages"
			],
			"problemMatcher": []
		}
	]
}